<% layout('panel') -%>
<div class="col-md-12">
    <div class="card card-default">
        <div class="card-body">
            <div class="card-header">
                <h3 class="card-title">
                    <%= title %>
                </h3>
            </div>
            <div class="row">
                <% console.log( notification )%>
                <!--    Alert        -->
                <% if (notification.Type === "Repair") { %>
                    <% include alert.ejs%>
                <% } %>
                <!--    Daily        -->
                <% if (notification.Type === "Daily") { %>
                    <% include daily.ejs%>
                <% } %>
                <!--    PPM        -->
                <% if (notification.Type === "PPM") { %>
                    <% include ppm.ejs%>
                <% } %>

            </div>
        </div>
    </div>
</div>